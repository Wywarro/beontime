{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "BeOnTime - API",
    "Audience": "BeOnTime - Clients"
  },
  "MongoDBOptions": {
    "ConnectionString": "mongodb://localhost",
    "Database": "beontime",
    "User": {
      "CollectionName": "Users",
      "ManageIndicies": true
    },
    "Role": {
      "CollectionName": "Roles",
      "ManageIndicies": true
    }
  },
  "ConnectionStrings": {
    "BEonTimeWebContextConnection": "Server=(localdb)\\mssqllocaldb;Database=BEonTime.Web;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}